  pmid = {21131448},
  keywords = {Adaptation,Evoked Potentials,force-field adaptation,Humans,Motor,motor control,Motor Cortex,Motor Cortex: physiology,Motor: physiology,Neuronal Plasticity,Neuronal Plasticity: physiology,Photic Stimulation,Photic Stimulation: methods,Physiological,Physiological: physiology,primary motor cortex,Psychomotor Performance,Psychomotor Performance: physiology,Random Allocation,Reaction Time,Reaction Time: physiology,repetition-dependent plasticity,transcranial magnetic stimulation,Transcranial Magnetic Stimulation: methods},
  file = {C\:\\Users\\jjorb\\Zotero\\storage\\7UWIXX5T\\Orban de Xivry et al. - 2011 - Contributions of the Motor Cortex to Adaptive Cont.pdf}
}

@article{orbandexivryDynamicRepresentationTarget2008,
  title = {A Dynamic Representation of Target Motion Drives Predictive Smooth Pursuit during Target Blanking.},
  author = {{Orban de Xivry}, Jean-Jacques and Missal, Marcus and Lef{\`e}vre, Philippe},
  year = {2008},
  month = jan,
  journal = {Journal of vision},
  volume = {8},
