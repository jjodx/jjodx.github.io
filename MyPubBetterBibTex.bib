@article{vandevoordeDoesProprioceptiveAcuity2021,
  title = {Does Proprioceptive Acuity Influence the Extent of Implicit Sensorimotor Adaptation in Young and Older Adults?},
  author = {Vandevoorde, Koenraad and {Orban de Xivry}, Jean-Jacques},
  year = {2021},
  month = aug,
  journal = {Journal of Neurophysiology},
  publisher = {{American Physiological Society}},
  issn = {0022-3077},
  doi = {10.1152/jn.00636.2020},
  urldate = {2021-09-15},
  abstract = {The ability to adjust movements to changes in the environment declines with aging. This age-related decline is caused by the decline of explicit adjustments. However, implicit adaptation remains intact and might even be increased with aging. Since proprioceptive information has been linked to implicit adaptation, it might well be that an age-related decline in proprioceptive acuity might be linked to the performance of older adults in implicit adaptation tasks. Indeed, age-related proprioceptive deficits could lead to altered sensory integration with an increased weighting of the visual sensory-prediction error. Another possibility is that reduced proprioceptive acuity results in an increased reliance on predicted sensory consequences of the movement. Both these explanations led to our preregistered hypothesis: we expected a relation between the decline of proprioception and the amount of implicit adaptation across ages. However, we failed to support this hypothesis. Our results question the existence of reliability-based integration of visual and proprioceptive signals during motor adaptation.},
  copyright = {All rights reserved},
  keywords = {aging,motor adaptation,proprioception,somatosensation},
  note = {preprint; data and code: https://osf.io/qg3t2/; preregistration: https://osf.io/qg3t2/registrations},
  file = {J\:\\GBW-0434_GroupOrban\\JJ\\Biblio\\2021\\Vandevoorde, Orban de Xivry - 2021 - Does proprioceptive acuity influence the extent of implicit sensorimotor.pdf}
}

