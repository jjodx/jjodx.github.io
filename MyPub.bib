@article{orban_de_xivry_dynamic_2008,
	title = {A dynamic representation of target motion drives predictive smooth pursuit during target blanking.},
	volume = {8},
	copyright = {All rights reserved},
	issn = {1534-7362},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/19146290},
	doi = {10.1167/8.15.6},
	abstract = {Moving objects are often occluded by neighboring objects. In order for the eye to smoothly pursue a moving object that is transiently occluded, a prediction of its trajectory is necessary. For targets moving on a linear path, predictive eye velocity can be regulated on the basis of target motion before and after the occlusions. However, objects in a more dynamic environment move along more complex trajectories. In this condition, a dynamic internal representation of target motion is required. Yet, the nature of such an internal representation has never been investigated. Similarly, the impact of predictive saccades on the predictive smooth pursuit response has never been considered. Therefore, we investigated the predictive smooth pursuit and saccadic responses during the occlusion of a target moving along a circular path. We found that the predictive smooth pursuit was driven by an internal representation of target motion that evolved with time. In addition, we demonstrated that in two dimensions, the predictive smooth pursuit system does influence the amplitude of predictive saccades but not vice versa. In conclusion, in the absence of retinal inputs, the smooth pursuit system is driven by the output of a short-term velocity memory that contains the dynamic representation of target motion.},
	number = {15},
	journal = {Journal of vision},
	author = {Orban de Xivry, Jean-Jacques and Missal, Marcus and Lefèvre, Philippe},
	month = jan,
	year = {2008},
	pmid = {19146290},
	keywords = {Adult, Humans, Pursuit, Saccades, Smooth, Photic Stimulation, Motion Perception, Motion Perception: physiology, Memory, Fixation, Ocular, Photic Stimulation: methods, Perceptual Masking, Short-Term, Short-Term: physiology},
	pages = {6.1--13},
	file = {Orban de Xivry et al. - 2008 - A dynamic representation of target motion drives p.pdf:C\:\\Users\\u0099946\\Zotero\\storage\\W33L98F9\\Orban de Xivry et al. - 2008 - A dynamic representation of target motion drives p.pdf:application/pdf}
}

