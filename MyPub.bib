	file = {Coppe et al. - 2012 - Dramatic impairment of prediction due to frontal l.pdf:C\:\\Users\\u0099946\\Zotero\\storage\\W93979TJ\\Coppe et al. - 2012 - Dramatic impairment of prediction due to frontal l.pdf:application/pdf}
